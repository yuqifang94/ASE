merge<-function(A,B,l){
C=c()#output array
#merge step
i=1
j=1
for(k in 1:l){
#end case A finishing
if(i>length(A)&j<=length(B)){
#copy rest B to end of array
C=c(C,B[j:length(B)])
} else if (j>length(B)&i<=length(A)){
#copy rest A to end of array
C=c(C,A[i:length(A)])
}else{
if(A[i]<B[j]){
C[k]=A[i]
i=i+1
}else{
C[k]=B[j]
j=j+1
}
}
}
return(C)
}
